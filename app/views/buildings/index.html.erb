<div class="home-container debugger abode-well-hero-background">

  <div class="hero-image debugger">
  </div>

  <div class="search-and-report-container abode-well-hero-container">
    <%= form_tag("/searches/", method: :post, class: "abode-well-hero-form") do %>
      <%= label_tag(:address, "Address:")%>
        <%= text_field_tag(:address)%>
      <%= label_tag(:zip_code, "Zip Code:")%>
        <%= text_field_tag(:zip_code)%>
      <%= submit_tag("Search")  %>
    <% end %>
  </div>

</div>


















</div> <!-- home-container end -->

